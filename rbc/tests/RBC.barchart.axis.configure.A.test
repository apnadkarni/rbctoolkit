namespace import rbc::*

package require tcltest
namespace import tcltest::*
namespace eval barchart.axis {
	# ------------------------------------------------------------------------------------
	# Purpose: Ensure the axis configure -bindtags command works correctly when given a 
	# single tag name.
	# ------------------------------------------------------------------------------------
	test RBC.barchart.axis.configure.A.1.1 {
		Bindtags - single tag
	} -setup {
		barchart .barchart1
		.barchart1 axis bind Binding1 <Double-1> {puts foobar}
		.barchart1 axis create Axis1
	} -body {
		.barchart1 axis configure Axis1 -bindtags Binding1
		.barchart1 axis cget Axis1 -bindtags
	} -cleanup {
		destroy Binding1
		.barchart1 axis delete Axis1
		destroy .barchart1
	} -result {Binding1}
	
	# ------------------------------------------------------------------------------------
	# Purpose: Ensure the axis configure -bindtags command works correctly when given  
	# a list of tag names.
	# ------------------------------------------------------------------------------------
	test RBC.barchart.axis.configure.A.1.2 {
		Bindtags - tag list
	} -setup {
		barchart .barchart1
		.barchart1 axis bind Binding1 <Double-1> {puts foo}
		.barchart1 axis bind Binding2 <Double-3> {puts bar}
		.barchart1 axis create Axis1
	} -body {
		.barchart1 axis configure Axis1 -bindtags {Binding1 Binding2}
		.barchart1 axis cget Axis1 -bindtags
	} -cleanup {
		destroy Binding1 Binding2
		.barchart1 axis delete Axis1
		destroy .barchart1
	} -result {Binding1 Binding2}
	
