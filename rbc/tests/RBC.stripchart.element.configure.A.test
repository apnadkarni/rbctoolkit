	# ------------------------------------------------------------------------------------
	# Purpose: Ensure the element configure -activepen command works correctly when given 
	# a valid pen name.
	# ------------------------------------------------------------------------------------
	test RBC.stripchart.element.configure.A.1.1 {
		stripchart element configure: activepen - valid pen name
	} -setup {
		stripchart .stripchart1
		.stripchart1 element create Element1
		.stripchart1 pen create Pen1 -color blue
	} -body {
		.stripchart1 element configure Element1 -activepen Pen1
		.stripchart1 element cget Element1 -activepen
	} -cleanup {
		.stripchart1 pen delete Pen1
		.stripchart1 element delete Element1
		destroy .stripchart1
	} -result {Pen1}
	
	# ------------------------------------------------------------------------------------
	# Purpose: Ensure the element configure -activepen command works correctly when given 
	# an invalid pen name.
	# ------------------------------------------------------------------------------------
	test RBC.stripchart.element.configure.A.1.2 {
		stripchart element configure: activepen - invalid pen name
	} -setup {
		stripchart .stripchart1
		.stripchart1 element create Element1
	} -body {
		if {[catch {.stripchart1 element configure Element1 -activepen invalid}]} {
			return 0
		} else {
			return 1
		}
	} -cleanup {
		.stripchart1 element delete Element1
		destroy .stripchart1
	} -result {0}
	
